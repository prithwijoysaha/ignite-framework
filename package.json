{
	"name": "ignite-framework",
	"version": "1.0.0",
	"private": true,
	"description": "An express based nodejs framework",
	"main": "./bin/www",
	"repository": {
		"type": "git",
		"url": "https://github.com/prithwijoysaha/ignite-framework"
	},
	"type": "module",
	"engines": {
		"npm": ">=8.19.2",
		"node": ">=18.12.1"
	},
	"scripts": {
		"prestart": "node ./bin/pre-start",
		"start": "node ./bin/www",
		"dev": "npx nodemon ./bin/www",
		"dev:debug": "DEBUG=* npx nodemon ./bin/www",
		"test": "jest --config ./jest.config.json",
		"test:watch": "npm run test -- --watch",
		"prepare": "husky install",
		"lint": "eslint .",
		"lint:fix": "npm run lint:js -- --fix",
		"prettier": "prettier --write .",
		"migrate:up": "npx sequelize-cli db:migrate",
		"migrate:undo": "npx sequelize-cli db:migrate:undo",
		"hydra:install": "npm i -g ./cli",
		"upgrade:latest": "ncu --upgrade",
		"audit:fix": "npm audit fix"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"keywords": [],
	"author": "Prithwijoy Saha",
	"license": "MIT",
	"devDependencies": {
		"commander": "^11.1.0",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jest": "^27.6.3",
		"eslint-plugin-jsdoc": "^48.0.2",
		"husky": "^8.0.3",
		"inquirer": "^9.2.12",
		"jest": "^29.7.0",
		"jsdoc": "^4.0.2",
		"jshint": "^2.13.6",
		"lint-staged": "^15.2.0",
		"nodemon": "^3.0.3",
		"npm-check-updates": "^16.14.12",
		"prettier": "^3.2.4",
		"sequelize-cli": "^6.6.2"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.9.14",
		"@joi/date": "^2.1.0",
		"async": "^3.2.5",
		"axios": "^1.6.5",
		"chalk": "^5.3.0",
		"change-case": "^5.4.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"dotenv-expand": "^10.0.0",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"express-actuator": "^1.8.4",
		"express-jwt": "^8.4.1",
		"express-rate-limit": "^7.1.5",
		"handlebars": "^4.7.8",
		"helmet": "^7.1.0",
		"http-errors": "^2.0.0",
		"ioredis": "^5.3.2",
		"ip": "^2.0.0",
		"joi": "^17.12.0",
		"jwks-rsa": "^3.1.0",
		"moment-timezone": "^0.5.44",
		"mongoose": "^8.1.0",
		"morgan": "^1.10.0",
		"nodemailer": "^6.9.8",
		"os-utils": "^0.0.14",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"pluralize": "^8.0.0",
		"rate-limit-redis": "^4.2.0",
		"redis": "^4.6.12",
		"sequelize": "^6.35.2",
		"swagger-ui-express": "^5.0.0",
		"uuid": "^9.0.1",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^4.7.1"
	}
}
